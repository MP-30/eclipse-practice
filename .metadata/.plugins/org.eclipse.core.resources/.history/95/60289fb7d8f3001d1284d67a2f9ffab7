package increment_decrement;

public class If_else_string {

	public static void main(String[] args) {
		 String X = "hello";
		 String Y = "JAva";
		 if (X == Y) {
			 System.out.println("The value of both string are equal");
			 
		 }
		 else {
			 System.out.println("value of both string are not equal");
		 }
	}
}
