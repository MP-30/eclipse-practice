package array;

public class Drawbacks_ofArray {

	public static void main(String[]aditya) {
	
		int nums[] = new int[6];
//		Fixed size is the drawback of array
		nums[0] = 15;
		nums[1] = 85;
		nums[2] = 57;
		nums[3] = 56;
		
		for (int i = 0; i <nums.length;i++) {
			System.out.println(nums[i]);
		}
	}
}
