package static_keywords;

class Mobile {
	String brand;
	int price;
	String name;
}


public class Basics {

	public static void main (String a[]) {
		
		Mobile mb = new Mobile();
		mb.brand = "Apple";
		mb.price = 56;
		mb.name = "SmartPhone";
		
		Mobile mb1 = new Mobile();
		mb.brand = "Samsung";
		mb.price = 56;
		mb.name = "SmartPhone";
		
		
		System.out.println(mb.brand);
	}
}
