package inhertance_method_overriding_polym;

public class Two_wheeler_father extends Moterbike_Grandparent {

	public void twostrokeEngine() {
		System.out.println("Parent's engine");
	}
	
	public void dualExhaustSystem() {
		System.out.println("Parent's dual exhaust system");
	}
	
	public void wheels() {
		System.out.println("Parent's wheel");
	}
}
