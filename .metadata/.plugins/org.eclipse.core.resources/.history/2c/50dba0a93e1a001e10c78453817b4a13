package coreJavaBrushUp;

public class Arrays {

	public static void main(String...strings) {
		 int[] Ar = new int[5];
		 Ar[0] = 4;
		 Ar[1] = 2;
		 Ar[2] = 8;
		 Ar[3] = 7;
		 Ar[4] = 6;
		 
		 int[] ar = {5,8,6,2,7,8};
		 
		 
		 
		 for(int i=0;i< ar.length;i++) {
			 System.out.println(ar[i]);
		 }
	}
}
